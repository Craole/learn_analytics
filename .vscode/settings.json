{
    "code-runner.executorMap": {
        "rust": "cd $dir && cargo run $fileName && $dir$fileNameWithoutExt",
    },
    "editor.formatOnSave": true,
    "rust-analyzer.check.overrideCommand": "clippy",
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "files.associations": {
        "*.vim": "viml",
        "nu": "nu",
        "*.fish*": "fish",
        "jgmenurc": "shellscript",
        "**/.config/git/config": "properties",
        "gitignore": "ignore",
        "*.py*": "python",
        "config": "properties",
        "**/Fonts/**/*.conf": "xml",
        "sxhdrc": "sxhkdrc",
        "**/sxhkd/**": "sxhkdrc",
        "*.toml": "toml",
        "**/Environment/context/**": "dotenv",
        ".envrc": "dotenv",
        "*.md": "markdown",
        "*rust-toolchain*": "toml",
        "*.pgsql": "sql",
        "*.rs": "rust",
        "rs": "rust",
        ".config": "shellscript"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Thrust Yellow Wash",
    "rust-analyzer.linkedProjects": [
        "./Cargo.toml",
        "./Cargo.toml",
        "./Cargo.toml",
        "./Cargo.toml",
        "./Cargo.toml"
    ],
    "rust-analyzer.showUnlinkedFileNotification": false,
}